	<!-- NumberOfEntries parameters to object mappings -
	     cwmpdmtoc tries to guess this from parameter names, these are just exceptions -->
	<counters>
		<counter name="ForwardNumberOfEntries" ref="Forwarding"/>
		<counter name="LANEthernetInterfaceNumberOfEntries" ref="LANEthernetInterfaceConfig"/>
		<counter name="LANUSBInterfaceNumberOfEntries" ref="LANUSBInterfaceConfig"/>
		<counter name="LANWLANConfigurationNumberOfEntries" ref="WLANConfiguration"/>
		<counter name="WANConnectionNumberOfEntries" ref="WANConnectionDevice"/>
	</counters>

	<!-- List of selector parameters - there's no other way for cwmpdmtoc to know whether
	     a parameter is of type T_SELECTOR or not -->
	<selectors>
		<selector path="InternetGatewayDevice.Layer3Forwarding." name="DefaultConnectionService"/>
		<selector path="InternetGatewayDevice.Layer3Forwarding.Forwarding.{i}." name="Interface"/>
